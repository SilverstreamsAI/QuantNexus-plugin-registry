{
  "id": "com.quantnexus.ai-strategy-analyzer",
  "name": "AI Strategy Analyzer",
  "description": "ML-powered strategy analysis and optimization. Uses machine learning models to identify patterns, optimize parameters, and predict strategy performance.",
  "author": {
    "name": "AlgoDev",
    "github": "algodev",
    "email": "dev@algodev.io"
  },
  "repository": "https://github.com/algodev/qn-ai-analyzer",
  "license": "Apache-2.0",
  "categories": ["ai", "analysis"],
  "pricing": {
    "type": "free",
    "premiumUrl": null
  },
  "permissions": {
    "network": true,
    "filesystem": "user",
    "native": false
  },
  "ui": {
    "panel": {
      "title": "AI Analyzer",
      "position": "main",
      "inputs": [
        { "id": "model", "type": "select", "label": "Model", "options": ["random_forest", "xgboost", "neural_net"], "default": "xgboost" },
        { "id": "lookback", "type": "number", "label": "Lookback Period", "default": 100 }
      ]
    }
  },
  "logic": {
    "entry": "src/analyzer.py",
    "runtime": "python",
    "worker": true
  },
  "dependencies": {
    "python": ["scikit-learn>=1.0.0", "xgboost>=1.5.0", "numpy>=1.20.0", "pandas>=1.3.0"],
    "node": {}
  },
  "pluginDependencies": {},
  "versions": [
    {
      "version": "0.9.0",
      "releaseDate": "2026-01-03",
      "downloadUrl": "https://github.com/algodev/qn-ai-analyzer/releases/download/v0.9.0/plugin.zip",
      "sha256": "c3d4e5f6789012345678901234567890abcdef1234567890abcdef1234567890",
      "minEngine": "0.1.0",
      "changelog": "Beta release with XGBoost and Random Forest models."
    }
  ]
}
