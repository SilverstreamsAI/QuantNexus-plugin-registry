{
  "id": "com.quantnexus.advanced-charts",
  "name": "Advanced Charts",
  "description": "Professional charting with 50+ technical indicators including RSI, MACD, Bollinger Bands, and more. Features customizable themes and real-time updates.",
  "author": {
    "name": "ChartMaster",
    "github": "chartmaster",
    "email": "contact@chartmaster.dev"
  },
  "repository": "https://github.com/chartmaster/qn-advanced-charts",
  "license": "MIT",
  "categories": ["visualization", "indicators"],
  "pricing": {
    "type": "free",
    "premiumUrl": null
  },
  "permissions": {
    "network": false,
    "filesystem": "plugin",
    "native": false
  },
  "ui": {
    "panel": {
      "title": "Chart Settings",
      "position": "sidebar",
      "inputs": [
        { "id": "period", "type": "number", "label": "Period", "default": 14 },
        { "id": "theme", "type": "select", "label": "Theme", "options": ["dark", "light"], "default": "dark" }
      ]
    }
  },
  "logic": {
    "entry": "src/index.py",
    "runtime": "python",
    "worker": true
  },
  "dependencies": {
    "python": ["numpy>=1.20.0", "pandas>=1.3.0"],
    "node": {}
  },
